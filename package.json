    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "prettier": "3.2.2",
    "tailwindcss": "^3.4.1",
